{
    "name": "logviewer/logviewer",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Logviewer\\Logviewer\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Logviewer\\Logviewer\\LogServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --provider=\"Logviewer\\Logviewer\\LogServiceProvider\" --tag=public --force",
        ],
    }
}