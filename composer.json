{
    "name": "logviewer/logviewer",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^7.4|^8.0|^8.1|^8.2|^8.3"
    },
    "autoload": {
        "psr-4": {
            "Logviewer\\Logviewer\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Logviewer\\Logviewer\\LogServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --tag=public --force"

        ]
    }
}